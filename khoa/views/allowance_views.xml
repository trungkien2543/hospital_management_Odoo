<odoo>
    <record id="view_benhvien_tienluong_phucap_tree" model="ir.ui.view">
        <field name="name">benhvien.tienluong.phucap.tree</field>
        <field name="model">benhvien.tienluong.phucap</field>
        <field name="arch" type="xml">
            <list string="Phụ cấp">
                <field name="code"/>
                <field name="name"/>
                <field name="amount"/>
            </list>
        </field>
    </record>

    <record id="view_benhvien_tienluong_phucap_form" model="ir.ui.view">
        <field name="name">benhvien.tienluong.phucap.form</field>
        <field name="model">benhvien.tienluong.phucap</field>
        <field name="arch" type="xml">
            <form string="Phụ cấp">
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                        <field name="amount"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
