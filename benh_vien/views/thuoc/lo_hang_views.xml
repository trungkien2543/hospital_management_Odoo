<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_lo_hang_form" model="ir.ui.view">
        <field name="name">hospital.lo_hang.form</field>
        <field name="model">hospital.lo_hang</field>
        <field name="arch" type="xml">
            <form string="Lô Hàng">
                <sheet>
                    <group>
                        <group>
                            <field name="ma_lo_hang"/>
                            <field name="thuoc"/>
<!--                            <field name="phieu_nhap"/>-->
                            <field name="ngay_nhap"/>
                            <field name="han_su_dung"/>
                        </group>
                        <group>
                            <field name="gia_nhap"/>
                            <field name="gia_ban"/>
                            <field name="trang_thai"/>
                            <field name="tinh_trang_chat_luong"/>
                            <field name="currency_id"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_lo_hang_list" model="ir.ui.view">
        <field name="name">hospital.lo_hang.list</field>
        <field name="model">hospital.lo_hang</field>
        <field name="arch" type="xml">
            <list string="Lô Hàng">
                <field name="ma_lo_hang"/>
                <field name="thuoc"/>
<!--                <field name="phieu_nhap"/>-->
                <field name="ngay_nhap"/>
                <field name="han_su_dung"/>
                <field name="gia_nhap"/>
                <field name="gia_ban"/>
                <field name="trang_thai"/>
                <field name="currency_id"/>
            </list>
        </field>
    </record>



</odoo>