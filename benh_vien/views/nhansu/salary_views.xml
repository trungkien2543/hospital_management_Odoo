<odoo>
    <record id="view_benhvien_tienluong_list" model="ir.ui.view">
        <field name="name">benhvien.tienluong.list</field>
        <field name="model">benhvien.tienluong</field>
        <field name="arch" type="xml">
            <list string="Bảng Lương">
                <field name="code"/>
                <field name="employee_code"/>
                <field name="month_year"/>
                <field name="working_days"/>
                <field name="overtime_hours"/>
                <field name="base_salary"/>
                <field name="salary_coefficient"/>
                <field name="allowance"/>
                <field name="tax_income"/>
                <field name="note"/>
            </list>
        </field>
    </record>

    <record id="view_benhvien_tienluong_form" model="ir.ui.view">
        <field name="name">benhvien.tienluong.form</field>
        <field name="model">benhvien.tienluong</field>
        <field name="arch" type="xml">
            <form string="Bảng Lương">
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="employee_code"/>
                        <field name="month_year"/>
                        <field name="working_days"/>
                        <field name="overtime_hours"/>
                        <field name="base_salary"/>
                        <field name="salary_coefficient"/>
                        <field name="allowance"/>
                        <field name="tax_income"/>
                        <field name="note"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    
    <menuitem id="menu_benhvien_tienluong_root" name="Lương, phụ cấp"/>


    


</odoo>
