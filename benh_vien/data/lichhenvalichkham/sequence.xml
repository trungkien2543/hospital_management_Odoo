<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence cho mã lịch hẹn -->
        <record id="seq_lichhen" model="ir.sequence">
            <field name="name">Mã lịch hẹn</field>
            <field name="code">benhvien.lichhen</field>
            <field name="prefix">LH-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho mã lịch khám -->
        <record id="seq_lichkham" model="ir.sequence">
            <field name="name">Mã lịch khám</field>
            <field name="code">benhvien.lichkham</field>
            <field name="prefix">LK-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
