<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="kehoach_code_incre" model="ir.sequence">
        <field name="name">Ke hoach Code</field>
        <field name="code">benhvien.kehoachdieutri</field>
        <field name="prefix">KH</field>  <!-- Tiền tố (có thể thay đổi) -->
        <field name="padding">3</field>  <!-- Độ dài số (MED0001, MED0002, ...) -->
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="giuongbenh_code_incre" model="ir.sequence">
        <field name="name">Tien Trinh Code</field>
        <field name="code">benhvien.tientrinh</field>
        <field name="prefix">TT</field>  <!-- Tiền tố (có thể thay đổi) -->
        <field name="padding">3</field>  <!-- Độ dài số (MED0001, MED0002, ...) -->
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
