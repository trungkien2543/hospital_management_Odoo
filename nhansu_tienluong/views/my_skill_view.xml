<odoo>
    <record id="view_benhvien_nhansu_kynang_list" model="ir.ui.view">
        <field name="name">benhvien.nhansu.kynang.list</field>
        <field name="model">benhvien.nhansu.kynang</field>
        <field name="arch" type="xml">
            <list string="Kỹ năng">
                <field name="title"/>
                <field name="skill_type"/>
                <field name="level"/>
                <field name="employee_id"/>
            </list>
        </field>
    </record>

    <record id="view_benhvien_nhansu_kynang_form" model="ir.ui.view">
        <field name="name">benhvien.nhansu.kynang.form</field>
        <field name="model">benhvien.nhansu.kynang</field>
        <field name="arch" type="xml">
            <form string="Kỹ năng">
                <sheet>
                    <group>
                        <field name="title"/>
                        <field name="skill_type"/>
                        <field name="level"/>
                        <field name="employee_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
