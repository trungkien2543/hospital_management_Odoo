<odoo>
    <record id="view_my_skill_list" model="ir.ui.view">
        <field name="name">my.skill.list</field>
        <field name="model">my.skill</field>
        <field name="arch" type="xml">
            <list string="Skills">
                <field name="title"/>
                <field name="skill_type"/>
                <field name="level"/>
                <field name="employee_id"/>
            </list>
        </field>
    </record>

    <record id="view_my_skill_form" model="ir.ui.view">
        <field name="name">my.skill.form</field>
        <field name="model">my.skill</field>
        <field name="arch" type="xml">
            <form string="Skill">
                <sheet>
                    <group>
                        <field name="title"/>
                        <field name="skill_type"/>
                        <field name="level"/>
                        <field name="employee_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
